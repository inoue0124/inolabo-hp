    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/typography": "^0.16.4",
    "@types/typography-breakpoint-constants": "^0.16.0",
